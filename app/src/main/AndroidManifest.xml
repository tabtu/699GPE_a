<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="uow.csse.tv.gpe">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.GET_TASKS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_color"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo_color"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:theme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.venue.VenueDetailActivity"/>
        <activity android:name=".activity.venue.VenueActivity"/>
        <activity android:name=".activity.UserDetailActivity"/>
        <activity android:name=".activity.club.SchoolActivity"/>
        <activity android:name=".activity.club.SchoolDetailActivity"/>
        <activity android:name=".activity.club.ClubActivity"/>
        <activity android:name=".activity.club.ClubDetailActivity"/>
        <activity android:name=".activity.NewsDetailActivity"/>
        <activity android:name=".activity.venue.VenueNewsDetailActivity"/>
        <activity android:name=".activity.RegisterActivity"/>
        <activity android:name=".activity.msg.MessageReceiveDetailActivity"/>
        <activity android:name=".activity.msg.MessageSendDetailActivity"/>
        <activity android:name=".activity.msg.MessageSendActivity"/>
        <activity android:name=".activity.act.MainActivityActivity"/>
        <activity android:name=".activity.FollowActivity"/>
        <activity android:name=".activity.act.ActivityDetailActivity"/>
        <activity android:name=".activity.club.ClubMemberActivity"/>

    </application>

</manifest>